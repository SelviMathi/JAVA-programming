import java.util.Scanner;

public class Main
{
    static void PrintString(String str, int n)
    {
        for(int i = 0; i < n; i++)
            System.out.print(str);
    }
    
    public static void main(String[] args) 
    {
        Scanner s = new Scanner(System.in);
        String str = s.next();
        int num = s.nextInt();
        s.nextLine(); 
        PrintString(str, num);
    }
}
