1.Print each character in string

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
		System.out.println(c);
	    }
	    
	}
}

2.ASCII
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
		System.out.println(c+" "+(int)c);
	    }
	    
	}
}

3.UPPERCASE

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isUpperCase(c))
		System.out.println(c);
	    }
	    
	}
}

4.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isLowerCase(c))
		System.out.println(c);
	    }
	    
	}
}

5.is letter

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    int count=0;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isLetter(c))
		count++;
	    }
	    System.out.println(count);
	}
}
6.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    int count=0;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isUpperCase(c))
		count++;
	    }
	    System.out.println(count+"UPPER");
	}
}

7.import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String res="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isUpperCase(c))
		res+=c;
	    }
	    System.out.println(res);
	}
}
