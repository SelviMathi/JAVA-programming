import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isLowerCase(c))
		System.out.println(c);
	    }
	    
	}
}

5.is letter

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    int count=0;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isLetter(c))
		count++;
	    }
	    System.out.println(count);
	}
}
6.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    int count=0;
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isUpperCase(c))
		count++;
	    }
	    System.out.println(count+"UPPER");
	}
}

7.import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String res="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isUpperCase(c))
		res+=c;
	    }
	    System.out.println(res);
	}
}
8.import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String res="",res1="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isDigit(c))
		res+=c;
		else
		res1+=c;
	    }
	    String finalStr= res + res1;
	    System.out.println(finalStr);
	}
}
9.import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String res="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(!(Character.isWhitespace(c)))
		res+=c;
			    }
	    
	    System.out.println(res);
	}
}

10.import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String res="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    if(Character.isUpperCase(c))
		    res+=Character.toLowerCase(c);
		    else
		    res+=Character.toUpperCase(c);
		   }
		   System.out.println(res);
	}
}

11.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String rev="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
	    rev=c+rev;
	    }
	    if(str.equalsIgnoreCase(rev))
	    System.out.println("Palindrome");
	    else
	     System.out.println("Not Palindrome");

	}
}

12.import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char c;
	    String res="";
	    Scanner obj=new Scanner (System.in);
	    String str=obj.nextLine();
	    for(int i=0;i<str.length();i++)
	   { 
	    c=str.charAt(i);
        if(c=='Y'||c=='y'||c=='z'||c=='Z')
        c-=26;
        c+=2;
        res+=c;
	    }
	    System.out.println(res);

	}
}


13.import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        char c;
        String word = ""; 
        Scanner obj = new Scanner(System.in);
        String str = obj.nextLine() + " "; // Add a space at the end of the input string
        
        for (int i = 0; i < str.length(); i++) {
            c = str.charAt(i);
            if (c != ' ') {
                word += c;
            } else {
                System.out.print(word);
                word = "";
            }
        }
    }
} or
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        char c;
        String word = ""; 
        Scanner obj = new Scanner(System.in);
        String str = obj.nextLine() + " "; 
        for (int i = 0; i < str.length(); i++) {
            c = str.charAt(i);
            if (!Character.isWhitespace(c)) {
                word += c;
            }                }
            
            System.out.print(word);
              
        
    }
}

